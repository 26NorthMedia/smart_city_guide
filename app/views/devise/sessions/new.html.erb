<section id="devise">
  <article class="objects">
  </article>
  <article class="form">
    <h2><%= t('sign_in') %></h2>
    <div>
      <%= form_for(resource, :as => resource_name, :url => user_session_path) do |f| %>
        <%= devise_error_messages! %>
        <%= f.label :email %>
        <%= f.email_field :email, :class => "form_tag" %>
        <%= f.label :password %>
        <%= f.password_field :password, :class => "form_tag" %>
        <% if devise_mapping.rememberable? -%>
          <div id="remember">
            <%= f.check_box :remember_me %> 
            <%= f.label :remember_me %>
          </div>
        <% end -%>
      <%= f.submit t('action.send'), :class =>"btn storePass" %>
      <% end %>
    </div>  
    <hr class="soften">
    <div class="options">
      <%= render :partial => "devise/shared/links" %>  
    </div>
  </article>
</section>
